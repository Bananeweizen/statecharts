package tests
testclass GuardedExit for statechart GuardedExit {
	@Test
	operation ExitTaken(){ 

		enter

		assert active(GuardedExit.main_region.A)
		assert ! guard
		raise e
		
		cycle

		assert active(GuardedExit.main_region.B)
		assert ! done 
	} 
	@Test
	operation ExitNotTaken(){
		
		enter 

		assert active(GuardedExit.main_region.A)
		guard = true
		raise e
		
		cycle

		assert active(GuardedExit.main_region.B)
		assert done 
	} 
		
}
	