
testgroup StringConcatenation for statechart StringConcatenation{
	test StringConcatenationTest { 
		enter
		assert active(StringConcatenation.main_region.A)
		
		cycle
		assert active(StringConcatenation.main_region.B)
		assert word == anotherword + "_test_" + "_23"
		
		raise myEvent : "reset"
		cycle
		assert active(StringConcatenation.main_region.C)
		assert anotherword == "reset"
		assert lastword == anotherword + "AS"
		 
	} 
}
	